<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddEmployee</servlet-name>
  	<display-name>AddEmployee</display-name>
  	<description></description>
  	<servlet-class>com.cap.ems.controller.AddEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AuthenticationService</servlet-name>
  	<display-name>AuthenticationService</display-name>
  	<description></description>
  	<servlet-class>com.cap.ems.service.AuthenticationService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddEmployee</servlet-name>
  	<url-pattern>/AddEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AuthenticationService</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
